AT_SETUP([Empty $Recycle.bin])
AT_KEYWORDS([recycledir])
AT_CHECK([$abs_top_builddir/src/rifiuti-vista -n $srcdir/samples/dir-empty],, [], [])
AT_CLEANUP

AT_SETUP([$Recycle.bin Sample 1])
AT_KEYWORDS([recycledir])
dnl AT_XFAIL_IF([true])
dnl dnl files are parsed in undefined order, need sorting
AT_CHECK([
		export LC_ALL=C
		sort $srcdir/samples/dir-sample1.txt > expout
		$abs_top_builddir/src/rifiuti-vista -n -8 $srcdir/samples/dir-sample1 | sort
],, [expout], [])
AT_CLEANUP

